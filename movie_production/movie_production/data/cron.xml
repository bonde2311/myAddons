<odoo>
    <record id="ir_cron_fetch_influencer_data" model="ir.cron">
        <field name="name">Fetch Social Influencer Data Daily</field>
        <field name="model_id" ref="model_movie_social_influencer"/>
        <field name="state">code</field>
        <field name="code">model.fetch_influencer_data()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="active">True</field>
        <field name="doall" eval="False"/>
    </record>
</odoo>
